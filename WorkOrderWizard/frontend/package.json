{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "npm ci --legacy-peer-deps && npm rebuild tailwindcss-animate && npm run build",
    "postinstall": "npx npm-force-resolutions && npm rebuild tailwindcss-animate",
    "prepare-vercel": "node ./scripts/prepare-vercel-build.js",
    "preinstall": "npm install -g npm-force-resolutions",
    "check-animate": "node -e \"try { require.resolve('tailwindcss-animate'); console.log('tailwindcss-animate is installed correctly'); } catch(e) { console.error('tailwindcss-animate NOT FOUND:', e.message); process.exit(1); }\""
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.0"
  },
  "resolutions": {
    "tailwindcss-animate": "^1.0.7"
  },
  "overrides": {
    "tailwindcss-animate": "^1.0.7"
  },
  "dependenciesMeta": {
    "tailwindcss-animate": {
      "built": false,
      "unplugged": true
    }
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "@tanstack/react-query": "^5.0.0",
    "react-hook-form": "^7.45.4",
    "zod": "^3.21.4",
    "@hookform/resolvers": "^3.3.1",
    "firebase": "^9.23.0",
    "axios": "^1.5.0",
    "lucide-react": "^0.288.0",
    "class-variance-authority": "^0.7.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "npm-force-resolutions": "^0.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
